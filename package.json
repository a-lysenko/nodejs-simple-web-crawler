{
  "name": "nodejs-simple-web-crawler",
  "version": "1.0.0",
  "dependencies": {
    "bunyan": "^1.8.12",
    "jsdom": "^11.6.2",
    "minimist": "^1.2.0"
  },
  "description": "A simple web crawler that locates a user-selected element on a web site with changing information",
  "main": "index.js",
  "devDependencies": {},
  "scripts": {
    "crowl-1": "node index.js ./pages/origin/sample-0-origin.html ./pages/other/sample-1-evil-gemini.html",
    "crowl-1:id": "node index.js ./pages/origin/sample-0-origin.html ./pages/other/sample-1-evil-gemini.html --id=make-everything-ok-button",
    "crowl-2": "node index.js ./pages/origin/sample-0-origin.html ./pages/other/sample-2-container-and-clone.html",
    "crowl-3": "node index.js ./pages/origin/sample-0-origin.html ./pages/other/sample-3-the-escape.html",
    "crowl-4": "node index.js ./pages/origin/sample-0-origin.html ./pages/other/sample-4-the-mash.html"
  },
